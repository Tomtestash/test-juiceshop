{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>Tomtestash/renovate-config"
  ],
  "addLabels": ["local label"],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all non-major dependencies name",
      "groupSlug": "all-minor-patch-slug"
    }
    ]
}
